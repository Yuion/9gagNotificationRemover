{
  "name": "9gagNotificationRemover",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Removes notifications on 9gag",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Remove"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ],
  "options_ui": {
    "page": "options.htm",
    "open_in_tab": false
  }
}